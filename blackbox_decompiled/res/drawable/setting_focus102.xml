<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="@color/colorPurpletrans" android:endColor="@color/colorPurpletrans" android:angle="0.0" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <gradient android:startColor="@color/colorPurpletrans" android:endColor="@color/colorPurpletrans" android:angle="0.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="@color/colorPurpletrans" android:endColor="@color/colorPurpletrans" android:angle="0.0" />
        </shape>
    </item>
    <item android:state_focused="false">
        <shape>
            <gradient android:startColor="@color/transparent" android:endColor="@color/transparent" android:angle="0.0" />
        </shape>
    </item>
</selector>
