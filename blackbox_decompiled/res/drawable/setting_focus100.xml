<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="@color/colorPrimayDark" android:endColor="@color/colorPrimayDark" android:angle="0.0" />
            <corners android:radius="26.0dip" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <gradient android:startColor="@color/colorPrimayDark" android:endColor="@color/colorPrimayDark" android:angle="0.0" />
            <corners android:radius="26.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="@color/colorPrimayDark" android:endColor="@color/colorPrimayDark" android:angle="0.0" />
            <corners android:radius="26.0dip" />
        </shape>
    </item>
    <item android:state_focused="false">
        <shape>
            <gradient android:startColor="@color/colorMoreMoreDarkGrey" android:endColor="@color/colorMoreMoreDarkGrey" android:angle="0.0" />
            <corners android:radius="26.0dip" />
        </shape>
    </item>
</selector>
